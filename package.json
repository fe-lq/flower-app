  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.3",
    "@dcloudio/uni-automator": "3.0.0-alpha-3090920231215001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3090920231215001",
