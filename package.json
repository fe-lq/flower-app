    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3090920231215001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3090920231215001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3090920231215001",
    "@dcloudio/uni-mp-xhs": "3.0.0-alpha-3090920231215001",
